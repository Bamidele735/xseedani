{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": ["aos"]

  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Swiper/index.tsx", "components/signIn/index.js", "components/Carousel/SlugCarousel/media/imageByIndex.ts"],
  "exclude": ["node_modules"]
}